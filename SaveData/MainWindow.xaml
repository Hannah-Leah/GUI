<Window x:Class="SaveData.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SaveData"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="Tbl_Output" HorizontalAlignment="Center" Margin="0,26,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBox x:Name="Tbx_Input" HorizontalAlignment="Left" Margin="305,170,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="213" Height="77"/>
        <Button x:Name="Save" Content="Save" HorizontalAlignment="Left" Margin="305,267,0,0" VerticalAlignment="Top" Click="Save_Click"/>
        <Button x:Name="Load" Content="Load" HorizontalAlignment="Left" Margin="488,267,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Click="Load_Click"/>

    </Grid>
</Window>
